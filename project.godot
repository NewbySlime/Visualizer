; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="visualizer"
run/main_scene="res://scenes/MainWindow.tscn"
run/low_processor_mode=true
config/icon="res://images/icon.png"

[autoload]

IEditInterfaceCreate="*res://scenes/propset/IEditInterface_Create.tscn"
ErrorShowAutoload="*res://scenes/Autoload/ErrorShowAutoload.tscn"
ErrorBlockShowAutoload="*res://scenes/Autoload/ErrorBlockShowAutoload.tscn"

[debug]

settings/fps/force_fps=60

[display]

window/size/resizable=false
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[global]

frames=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
